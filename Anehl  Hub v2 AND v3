local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Nihasa Angel Hub V3?", 5013109572)

-- themes
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}

local page = venyx:addPage("Discord", 5012544693)
local section1 = page:addSection("Join My Discord Servers")
section1:addButton("LINK https://discord.gg/kN3Pd4tNYh", function()
print("Clicked")
end)

local page = venyx:addPage("Scripts", 5012544693)
local section1 = page:addSection("Games Scripts")

section1:addButton("Prisoner Life", function()
print("Clicked")
loadstring(game:HttpGet(('https://raw.githubusercontent.com/XTheMasterX/Scripts/Main/PrisonLife'),true))()
end)

local page = venyx:addPage("FE Scripts", 5012544693)
local section1 = page:addSection("Working Only R6 Games")


local theme = venyx:addPage("Theme", 5012544693)
local colors = theme:addSection("Colors For Hub")

for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
colors:addColorPicker(theme, color, function(color3)
venyx:setTheme(theme, color3)
end)
end

-- load
venyx:SelectPage(venyx.pages[1], true)
